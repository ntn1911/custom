//if you don't know what a parameter does, visit:
//https://developer.valvesoftware.com/wiki/$phong
"VertexLitGeneric"
{
    "$basetexture" "models\weapons\ub\fmg9\zuo"

 "$nocull"		"1"   //背面剔除（红色）
 "$additive" "1"  //黑色背景透明
 "$selfillum"   "1"
 "$selfillumtint" "0.2 0.2 0.2"  //亮度

Proxies
{
AnimatedTexture //动态贴图
{
animatedTextureVar $basetexture //以$basetexture贴图
animatedTextureFrameNumVar $frame //贴图的帧播放
animatedTextureFrameRate 19 //速率，越大越快，可可以小数点但要加引号
}
