"VertexlitGeneric"
{
$basetexture "models\weapons\v_models\cf_xiuluo\KNIFE_BORNBEAST_S"
$bumpmap "models\weapons\v_models\cf_xiuluo\KNIFE_BORNBEAST_N"

$phong 1
$phongexponent 	"50"
$phongboost "0.05"
$phongfresnelranges "[.8 .8 1]"
$phongalbedotint 1
$phongdisablehalflambert 1

$basealphaenvmapmask "1"
$envmap env_cubemap
$envmapfresnel 1
$envmapFresnelMinMaxExp "[.01 1 1]"
$envmaptint "[.5 .5 .5]"

$selfillum 1
"Proxies" 
		{ 
		"AnimatedTexture" 
			{ 
			"animatedTextureVar" "$basetexture" 
			"animatedTextureFrameNumVar" "$frame" 
			"animatedTextureFrameRate" "8" 
			}
		} 

}

